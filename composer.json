{
    "name": "pre/phpx",
    "type": "pre-compiler",
    "license": "MIT",
    "require": {
        "pre/plugin": "dev-feature/expressions",
        "nikic/php-parser": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "files": [
            "source/functions.php",
            "html-source/functions.php",
            "html-examples/functions.php"
        ],
        "psr-4": {
            "Pre\\Phpx\\": "source",
            "Pre\\Phpx\\Html\\": "html-source"
        }
    },
    "autoload-dev": {
        "files": ["tests/TestCase.php"]
    },
    "extra": {
        "compilers": ["Pre\\Phpx\\compile"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
